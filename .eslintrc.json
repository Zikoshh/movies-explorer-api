{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "consistent-return": "off",
    "operator-linebreak": ["error", "after"],
    "import/extensions": ["error", "ignorePackages"],
    "no-useless-escape": "off"
  }
}
